<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="android.testing.system.testcase">
	<class name="Testcase" table="TESTCASE" lazy="false">
		<id name="id" type="java.lang.Integer">
			<column name="ID" />
		</id>
		<property name="project_id" type="java.lang.Integer">
			<column name="PROJECT_ID" />
		</property>
		<property name="name" type="java.lang.String">
			<column name="NAME" />
		</property>
		<property name="testcase" type="java.sql.Blob">
			<column name="TESTCASE" />
		</property>
		<property name="create_time" type="java.lang.Long">
			<column name="CREATE_TIME" />
		</property>
		<property name="modify_time" type="java.lang.Long">
			<column name="MODIFY_TIME" />
		</property>
	</class>
</hibernate-mapping>