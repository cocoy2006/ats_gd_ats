<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="android.testing.system.project">
	<class name="Project" table="PROJECT" lazy="false">
		<id name="id" type="java.lang.Integer">
			<column name="ID" />
		</id>
		<property name="user_id" type="java.lang.Integer">
			<column name="USER_ID" />
		</property>
		<property name="name" type="java.lang.String">
			<column name="NAME" />
		</property>
		<property name="create_time" type="java.lang.Long">
			<column name="CREATE_TIME" />
		</property>
		<property name="modify_time" type="java.lang.Long">
			<column name="MODIFY_TIME" />
		</property>
	</class>
</hibernate-mapping>